{
  "name": "multi",
  "version": "2.0.0",
  "description": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/univlorraine/esup-multi.git"
  },
  "author": "UniversitÃ© de Lorraine",
  "license": "CeCILL-2.1",
  "bugs": {
    "url": "https://github.com/univlorraine/esup-multi/issues"
  },
  "homepage": "https://github.com/univlorraine/esup-multi#readme",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ci": "pnpm i --frozen-lockfile",
    "build:back": "pnpm --stream --parallel --filter \"./dev/user-backend-nest/**\" run build",
    "build:front": "pnpm --filter \"./dev/user-frontend-ionic/\" run module:build-all",
    "build:connectors": "pnpm --stream --filter \"./dev/connectors/*\" run build",
    "start:back": "pnpm --stream --parallel --filter \"./dev/user-backend-nest/**\" run start",
    "start:back:dev": "pnpm --stream --parallel --filter \"./dev/user-backend-nest/**\" run start:dev",
    "start:front": "pnpm --filter \"./dev/user-frontend-ionic\" ionic serve",
    "start:mocks": "pnpm --filter \"./dev/user-backend-mocks\" run start",
    "start:connectors": "pnpm --stream --parallel --filter \"./dev/connectors/*\" run start"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.10",
    "license-checker-rseidelsohn": "^4.3.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
